
alias ls="ls --color"
alias ll="ls -l --color"
alias la="ls -l -a --color"
alias more=less
alias view="vi -R"
export PS1='\[\e]0;\w\a\]\n\[\e[33m\]\u@\h \[\e[33m\]\w\[\e[0m\]\n\t\$ '

export PAGER=less
export LESS="-iMSx4 -FX"

# Quick history add
export PROMPT_COMMAND="history -a; history -c; history -r; $PROMPT_COMMAND"
export HISTCONTROL=ignoredups

# PG APP NAME
export PGAPPNAME=wiwwo